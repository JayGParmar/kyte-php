{
    "name": "keyqcloud/kyte-php",
    "description": "Light-weight framework for developing versatile PHP applications.",
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "aws/aws-sdk-php": "^3.101",
        "sendgrid/sendgrid": "^7.3",
        "stripe/stripe-php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "authors": [
        {
            "name": "Kenneth Hough",
            "email": "kenneth@keyqcloud.com"
        }
    ],
    "autoload": {
        "psr-4":
            {
                "Kyte\\": "src/"
            },
            "files": ["src/functions.php"]
    }
}